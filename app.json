{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "ALLOW ${PRODUCT_NAME} to acces your photos.",
          "savePhotosPermission": "ALLOW ${PRODUCT_NAME} to save photos.",
          "isAccessMediaLocationEnabled": "true"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      "expo-router"
    ],
    "version": "1.0.0",
    "icon": "./assets/icons/cloud-rain2.png",
    "name": "Sääohjelma",
    "slug": "weather-app",
    "description": "Tämä on sääohjelma sovellus",
    "orientation": "portrait",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0545247c-7c14-44f0-87bf-6bcffd6cd2dc"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "android": {
      "package": "com.tase64.weatherapp"
    },
    "ios": {}
  }
}
